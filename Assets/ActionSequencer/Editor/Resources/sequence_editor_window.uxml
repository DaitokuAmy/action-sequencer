                            <ActionSequencer.Editor.RangeSequenceEventView focusable="true" style="background-color: rgba(84, 90, 195, 255);" />
                        </ActionSequencer.Editor.SequenceTrackView>
                        <ActionSequencer.Editor.SequenceTrackView>
                            <ActionSequencer.Editor.SignalSequenceEventView focusable="true" style="background-color: rgba(17, 255, 0, 255);" />
                            <ActionSequencer.Editor.SignalSequenceEventView focusable="true" style="background-color: rgba(255, 16, 229, 255);" />
                        </ActionSequencer.Editor.SequenceTrackView>
                    </ui:VisualElement>
                </ui:ScrollView>
                <ui:VisualElement name="TrackSeekbar" class="track_seekbar" style="position: absolute; height: 100%; width: 2px;" />
