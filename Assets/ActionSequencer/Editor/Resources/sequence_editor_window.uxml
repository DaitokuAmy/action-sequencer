            <ui:ScrollView name="InspectorScrollView">
                <ActionSequencer.Editor.VisualElements.InspectorView text="Sample&#10;" style="padding-left: 4px; padding-right: 4px; padding-top: 4px; padding-bottom: 4px;" />
